{
  "name": "JSON Diff",
  "version": "0.2",
  "description": "JSON Diff chrome extension which works offline",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "icons": {
    "16": "json-diff-16.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "JSON Diff offline"
  }
}
